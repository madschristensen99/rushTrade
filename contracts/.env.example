# Monad Testnet RPC
MONAD_TESTNET_RPC_URL=https://testnet.monad.xyz

# Deployer wallet private key (DO NOT COMMIT THE REAL ONE)
DEPLOYER_PRIVATE_KEY=your_private_key_here

# Collateral token address (we'll need to deploy a test USDC or use existing)
COLLATERAL_TOKEN=0x0000000000000000000000000000000000000000

# Fee recipient address (can be deployer address)
FEE_RECIPIENT=your_address_here

# Protocol fee in basis points (50 = 0.5%, 100 = 1%)
PROTOCOL_FEE_BPS=50

# Optional: Use existing CTF deployment
# EXISTING_CTF=0x...
